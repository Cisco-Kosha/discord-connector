{
  "version": "2.0.0",
  "oauth2_provider_name": "discord",
  "fields": [
    {
      "field": "ACCESS_TOKEN",
      "displayName": "OAuth2 Access Token",
      "type": "String",
      "minLength": 50,
      "maxLength": 250,
      "required": true,
      "description": "Discord OAuth2 access token",
      "helpSection": "Discord OAuth2 access token"
    },
    {
      "field": "REFRESH_TOKEN",
      "displayName": "OAuth2 refresh token",
      "type": "String",
      "minLength": 10,
      "maxLength": 180,
      "required": false,
      "description": "Discord OAuth2 refresh token",
      "helpSection": "Discord OAuth2 refresh token"
    },
    {
      "field": "EXPIRES_AT",
      "displayName": "OAuth2 Expiry Time",
      "type": "String",
      "minLength": 5,
      "maxLength": 30,
      "required": false,
      "description": "Discord OAuth2 token refresh time",
      "example": "2022-11-18T15:08:55.642141+05:30",
      "helpSection": "Discord OAuth2 token refresh time"
    },
    {
      "field": "API_KEY",
      "displayName": "Discord API Key",
      "type": "String",
      "minLength": 4,
      "maxLength": 80,
      "required": true,
      "description": "Discord Token for the Kosha Bot",
      "example": "Bot NPfWE2p1H7yBzIXnRB",
      "default": "Bot MTA2MDg4OTMzNzM1NDUzOTA4OA.G4IhEa.otFB-3q-grSo8_FMlJBLLiOoDWiK_fCyeNL3JY",
      "helpSection": "Provide the Kosha Bot Token ID prefixed by Bot and a <space>"
    },
    {
      "field": "API_KEY_HEADER_NAME",
      "displayName": "Discord API Key Header Name",
      "type": "String",
      "minLength": 4,
      "maxLength": 60,
      "required": true,
      "description": "Discord API Key Header Name",
      "example": "Authorization",
      "default": "Authorization"
    },
    {
      "field": "AUTH_TYPE",
      "displayName": "Discord Auth Type",
      "type": "String",
      "minLength": 4,
      "maxLength": 60,
      "required": true,
      "description": "Discord API Auth Type",
      "default": "API_KEY",
      "example": "API_KEY",
      "helpSection": ""
    },
    {
      "field": "SERVER_URL",
      "displayName": "Discord Server URL",
      "type": "String",
      "minLength": 4,
      "maxLength": 60,
      "required": true,
      "description": "Discord API Server URL",
      "default": "https://discordapp.com/api",
      "example": "https://discordapp.com/api",
      "helpSection": "Use the default value https://discordapp.com/api"
    },
    {
      "field": "provider",
      "displayName": "OAuth2 Provider Name",
      "type": "String",
      "minLength": 1,
      "maxLength": 60,
      "required": true,
      "description": "Discord OAuth2 provider name",
      "example": "discord",
      "helpSection": "Discord OAuth2 provider name"
    },
    {
      "field": "email",
      "displayName": "Email fetched from OAuth2 auth flow",
      "type": "String",
      "minLength": 1,
      "maxLength": 60,
      "required": true,
      "description": "Discord email fetched from OAuth2 auth flow",
      "example": "bob@email.com",
      "helpSection": "Discord email fetched from OAuth2 auth flow"
    }
  ]
}
